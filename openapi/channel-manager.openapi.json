    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "x-api-key",
        "description": "Provide your API key in the x-api-key header"
      }
    },
    "schemas": {
      "CreateChannelIntegrationDto": {
        "type": "object",
        "properties": {
          "hotelId": {
            "type": "number",
            "description": "Hotel identifier",
            "example": 123
          },
          "channelType": {
            "enum": [
                  "BOOKING_COM",
                  "EXPEDIA",
                  "AIRBNB",
                  "HOTELS_COM",
                  "TRIPADVISOR",
                  "AGODA",
                  "HOTELBEDS",
                  "CUSTOM",
                  "SEVEN"
                ],
            "type": "string",
            "description": "Channel type (OTA)"
          },
          "channelName": {
            "type": "string",
            "description": "Human readable channel name",
            "example": "Booking.com"
          },
          "status": {
            "enum": [
              "ACTIVE",
              "INACTIVE",
              "ERROR",
              "PENDING",
              "TESTING"
            ],
            "type": "string"
          },
          "apiKey": {
            "type": "string",
            "description": "API key for the channel"
          },
          "apiSecret": {
            "type": "string",
            "description": "API secret for the channel"
          },
          "accessToken": {
            "type": "string"
          },
          "refreshToken": {
            "type": "string"
          },
          "channelPropertyId": {
            "type": "string",
            "description": "Property ID used by the channel"
          },
          "channelUsername": {
            "type": "string"
          },
          "channelPassword": {
            "type": "string"
          },
          "webhookUrl": {
            "type": "string",
            "description": "Webhook URL for channel events"
          },
          "webhookSecret": {
            "type": "string",
            "description": "Secret used to sign webhooks"
          },
          "isWebhookEnabled": {
            "type": "boolean"
          },
          "syncIntervalMinutes": {
            "type": "number",
            "description": "Interval in minutes for periodic sync"
          },
          "isRealTimeSync": {
            "type": "boolean",
            "description": "Enable real-time sync if supported"
          },
          "testMode": {
            "type": "boolean",
            "description": "Run integration in test/sandbox mode"
          },
          "channelSettings": {
            "type": "object",
            "additionalProperties": true
          },
          "supportedFeatures": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "hotelId",
          "channelType",
          "channelName"
        ]
      },
